{
  "name": "es6-library-starter",
  "version": "1.0.0",
  "description": "ES6 NPM Library Starter",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "npm build",
    "build": "webpack --progress --colors",
    "build-min": "PROD_DEV=1 webpack --progress --colors",
    "build-watch": "webpack --progress --colors --watch"
    "unit": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "unit-watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "test": "npm run unit-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liady/es6-library-starter.git"
  },
  "keywords": [
    "es6",
    "npm",
    "starter",
    "library",
    "boilerplate",
    "babel",
    "umd",
    "webpack"
  ],
  "author": "Liad Yosef",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liady/es6-library-starter/issues"
  },
  "homepage": "https://github.com/liady/es6-library-starter",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "mocha": "^2.3.4",
    "webpack": "^1.12.11"
  }
}
